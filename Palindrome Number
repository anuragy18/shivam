class Solution {
    public boolean isPalindrome(int x) {
       
        int o=x;
        int r=0;
        if(o<0){
                return false;
            }
        while(x!=0){
                int p=x%10;
                r=r*10+p;
                x/=10;    
        }
        if(o==r){
            return true;
        }
        else{
            return false;
        }
    }
}
